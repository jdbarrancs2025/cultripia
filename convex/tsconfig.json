{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "skipLibCheck": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "lib": ["ES2021"],
    "strict": true,
    "noEmit": true,
    "isolatedModules": true,
    "paths": {}
  },
  "include": ["./**/*"],
  "exclude": ["_generated"]
}